<Window x:Class="Client.View.Attachments.FileDownload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        ResxExtension.DefaultResxName="Client.View.Attachments.ResAttachment"
        Language="{UICulture}"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{Resx FileDownload}" Height="171" Width="480" Loaded="WindowLoaded" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Name="rootGrid">
        <ProgressBar Height="28" HorizontalAlignment="Left" Margin="27,31,0,0" Name="progressBar1" VerticalAlignment="Top" Width="391" />
        <Label Height="28" HorizontalAlignment="Left" Margin="425,30,0,0" Name="percent" VerticalAlignment="Top" />
        <Button Content="{Resx OpenFile}" Height="23" HorizontalAlignment="Left" Margin="108,88,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="Button1Click" />
        <Button Content="{Resx ResxName=Client.Properties.Resources, Key=Cancel}" Height="23" HorizontalAlignment="Left" Margin="274,88,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="Button2Click" />
    </Grid>
</Window>
